{
    "$schema": "https://www.schemastore.org/jscpd.json",
    "absolute": false,
    "blame": false,
    "exitCode": 0,
    "gitignore": true,
    "ignore": [
        "**/node_modules/**",
        "**/.git/**",
        "**/.github/**",
        "**/dist/**",
        "**/build/**",
        "**/coverage/**",
        "**/report/**",
        "**/docs/**",
        "**/html/**",
        "**/assets/**",
        "**/icons/**",
        "**/release/**",
        "**/benchmarks/**",
        "**/scripts/**",
        "**/libs/**",
        "**/test/**",
        "**/tests/**",
        "**/src/test/**",
        "**/electron/test/**",
        "**/vscode/**",
        "**/*.d.ts",
        "**/*.test.ts",
        "**/*.spec.ts",
        "**/*.test.tsx",
        "**/*.spec.tsx",
        "**/*.config.ts",
        "**/*.config.js",
        "**/*.config.mjs",
        "**/*.json",
        "**/*.md",
        "**/*.xml",
        "**/*.html",
        "**/*.css",
        "**/*.scss",
        "**/*.sass",
        "**/*.svg",
        "**/*.png",
        "**/*.jpg",
        "**/*.jpeg",
        "**/*.gif",
        "**/*.ico",
        "**/*.woff",
        "**/*.woff2",
        "**/*.ttf",
        "**/*.eot",
        "**/*.zip",
        "**/*.tar",
        "**/*.gz",
        "**/*.bin",
        "**/*.wasm"
    ],
    "ignoreCase": false,
    "maxLines": 10000,
    "maxSize": "100kb",
    "minLines": 10,
    "minTokens": 50,
    "mode": "mild",
    "noSymlinks": false,
    "output": "./reports/jscpd/",
    "reporters": [
        "html",
        "markdown",
        "json",
        "console",
        "xml",
        "csv"
    ],
    "reportersOptions": {
        "badge": {
            "color": "purple",
            "iconWidth": 13,
            "label": "jscpd",
            "style": "flat"
        }
    },
    "silent": false,
    "skipLocal": false,
    "store": "leveldb",
    "threshold": 50,
    "verbose": false
}
